{
  "settings": {
    "services": {
      "experiment": {
        "exposed": true
      },
      "agent": {
        "exposed": true
      },
      "storage": {
        "exposed": true,
        "canPersist": true,
        "maxReplication": 3
      },
      "cms": {
        "exposed": true,
        "automatic": false,
        "predicateOnNewContext" : false,
        "predicateThread": {
          "initialDelay": 30,
          "period": 60

        },
        "policiesThread": {
          "initialDelay": 45,
          "period": 60
        },
        "checkPoliciesThread": {
          "initialDelay": 45,
          "period": 60
        },
        "getdataThread": {
          "initialDelay": 60,
          "period": 60
        },
        "spawnThread": {
          "initialDelay": 90,
          "period": 120
        }
      },
      "mds": {
        "exposed": true
      },
      "rms": {
        "exposed": true
      },
      "nds": {
        "exposed": true,
        "startupRegistration" : false
      },
      "mms": {
        "exposed": true
      }
    },
    "database": {
      "filename": "node.db"
    },
    "rest": {
      "port": 8080
    },
    "webDAV": {
      "port": 8081
    },
    "webAPP": {
      "port": 8082
    },
    "keys": {
      "location": "~/sos/keys/"
    },
    "store": {
      "type": "local",
      "location": "~/sos/"
    },
    "global": {
      "ssl_trust_store" : "/Library/Internet Plug-Ins/JavaAppletPlugin.plugin/Contents/Home/lib/security/cacerts",
      "tasks": {
        "thread": {
          "ps": 4
        }
      },
      "nodeMaintainer": {
        "enabled" : false,
        "maxSize": 1048576,
        "thread": {
          "ps": 1,
          "initialDelay": 0,
          "period": 600
        }
      }
    },
    "bootstrapNodes": []
  }
}